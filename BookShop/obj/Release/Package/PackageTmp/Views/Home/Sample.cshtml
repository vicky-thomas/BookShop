@model IEnumerable<BookShop.Models.ArticleName>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Sample</title>
</head>
<body>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ArticleName1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Book.BookName)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ArticleName1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Book.BookName)
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
